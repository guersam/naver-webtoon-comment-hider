{
  "manifest_version": 2,

  "name": "Naver Webtoon Comment Hider",
  "version": "0.1.0",

  "description": "This extension hides comments in Naver webtoon",

  "icons": {
      "128": "icon.png"
  },

  "content_scripts": [
    {
      "matches": ["*://comic.naver.com/ncomment/ncomment.nhn*"],
      "js": ["hider.js"],
      "all_frames": true
    }
  ]
}
